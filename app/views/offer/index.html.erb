<div class="row clearfix">
  <div class="col-md-12 column">
    <div class="page-header">
      <h1>
        Fyber Offers Api Consumer
      </h1>
    </div>
    <%= simple_form_for :fyber, url: offer_show_path do |f| %>
        <div class="form-group">
          <label for="exampleInputEmail1">
            UID
          </label>
          <%= f.input :uid, label: false, input_html: {class: "form-control"} %>
        </div>
        <div class="form-group">
          <label for="exampleInputEmail1">
            PUB0
          </label>
          <%= f.input :pub0, label: false, input_html: {class: "form-control"}, required: false %>
        </div>
        <div class="form-group">
          <label for="exampleInputEmail1">
            PAGE
          </label>
          <%= f.input :page, label: false, input_html: {class: "form-control"}, required: false %>
        </div>

        <button type="submit" class="btn btn-primary btn-default">Submit</button>
    <% end %>


  </div>
</div>



